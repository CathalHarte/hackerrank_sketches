{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build: big_factorial",
            "type": "shell",
            "command": "mkdir -p build; cd build; cmake -DCMAKE_BUILD_TYPE=Debug ..; make",
            "options": {
                "cwd": "${workspaceFolder}/big_factorial"
            },
            "problemMatcher": {
                "base": "$gcc",
                "fileLocation": "absolute"

            },
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "label": "build: euler_245",
