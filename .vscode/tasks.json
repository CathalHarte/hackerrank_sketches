            "options": {
                "cwd": "${workspaceFolder}/euler_245"
            },
            "problemMatcher": {
                "base": "$gcc",
                "fileLocation": "absolute"
            },
            "group": {
                "kind": "build",
                "isDefault": true
            }
        }
    ]
