    "version": "0.2.0",
    "configurations": [
        

        {
            "name": "launch: debug big_factorial",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/big_factorial/build/big_factorial",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/big_factorial/build",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build: big_factorial",
            "miDebuggerPath": "/usr/bin/gdb"
        },

